# Install the EditorConfig VS plugin to automatically apply formatting settings to this solution
# See http://editorconfig.org/ for syntax

# top-most EditorConfig file
root = true

[*.cs]
indent_style = tab
indent_size = 4

[*.config]
indent_style = tab
indent_size = 2

[*.feature]
indent_style = tab
indent_size = 4

[*.js]
indent_style = tab
indent_size = 4

[*.sql]
indent_style = tab

[*.csproj]
indent_style = space
indent_size = 2

[*.{yaml,yml}]
indent_style = space
indent_size = 2